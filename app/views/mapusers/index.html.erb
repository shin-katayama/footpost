<p id="notice"><%= notice %></p>

<h1>Listing Mapusers</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mapusers.each do |mapuser| %>
      <tr>
        <td><%= mapuser.title %></td>
        <td><%= mapuser.description %></td>
        <td><%= mapuser.address %></td>
        <td><%= mapuser.latitude %></td>
        <td><%= mapuser.longitude %></td>
        <td><%= link_to 'Show', mapuser %></td>
        <td><%= link_to 'Edit', edit_mapuser_path(mapuser) %></td>
        <td><%= link_to 'Destroy', mapuser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mapuser', new_mapuser_path %>
